@{
    if (ViewBag.Icon == null)
    {
        ViewBag.Icon = "https://cdn11.bigcommerce.com/s-ww1fqjacln/product_images/ets-favicon.png?t=1591714686";
    }
}
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="google-signin-client_id" content="228753140869-9kigcbgkcrcbm3ktdppnq26aq0jfi8lb.apps.googleusercontent.com">
    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
    <title>@ViewBag.Title</title>
    @*<script src="https://code.jquery.com/jquery-1.12.4.js"></script>
    <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.10.3/jquery-ui.min.js"></script>*@
    <link rel="icon" type="image/png" href="@ViewBag.Icon" sizes="32x32">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css" integrity="sha384-MCw98/SFnGE8fJT3GXwEOngsV7Zt27NXFoaoApmYm81iuXoPkFOJwJ8ERdknLPMO" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
</head>

<body style="background-color:#ffffff">
    <script src="https://apis.google.com/js/platform.js" async defer></script>
    @RenderPage("~/Views/Shared/Header.cshtml")
    @RenderPage("~/Views/Shared/ModalUserInfo.cshtml")
    @RenderPage("~/Views/Shared/ModalMessage.cshtml")
    <script>
        const hideMessage = () => {
            $("#messageModal").modal('hide');
        }
        const showMessage = () => {
            $("#messageModal").modal('show');
        }
    </script>
    <div class="container pt-2" style="background-color:white;">
        @RenderBody()
    </div>
    <br />
    <br />
    <footer class="blockquote-footer">
        Số lượng người đã truy cập @ApplicationInstance.Application["Views"]
        <div class="d-flex justify-content-center">
            ©2021 SCIMTA
        </div>
        <div class="d-flex justify-content-center">

            <a href="https://github.com/SCIMTA">
                <ion-icon name="logo-github" size="large" style="padding:5px;"></ion-icon>
            </a>
        </div>

    </footer>
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"></script>
    <script src="https://unpkg.com/ionicons@5.4.0/dist/ionicons.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>

    @*<script src="https://unpkg.com/ionicons@5.4.0/dist/ionicons.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.3/umd/popper.min.js" integrity="sha384-ZMP7rVo3mIykV+2+9J3UJ46jBk0WLaUAdn689aCwoqbBJiSnjAK/l8WvCWPIPm49" crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js" integrity="sha384-ChfqqxuZUCnJSK3+MXmPNIyE6ZbWh2IMqE241rYiqJxyMiZ6OW/JmZQ5stwEULTy" crossorigin="anonymous"></script>*@

    <script>
        if (document
            .getElementById("message").textContent.trim().length > 0) {
            console.log("run")
            setTimeout(() => {
                showMessage();
            }, 300);
        }
        const goToLoginPage = () => {
            window.location.href = `/Home/Login?returnUrl=${window.location.href}`
        }
    </script>
</body>
</html>